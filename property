# -*- coding:utf-8 -*-
#propertyç”¨æ³•2
class Test(object):
    def __init__(self):
        self.__num=200
        
    @property
    def num(self):
        print("___",self.__num)
        return self.__num
    
    @num.setter
    def num(self, newnum):
        if self.__num>=0:
            self.__num = newnum-1
        else:
            self.__num==0
t=Test()
t.num = 100
print(t.num)
